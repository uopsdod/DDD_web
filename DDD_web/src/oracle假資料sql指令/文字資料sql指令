/* Banner */
CREATE TABLE Ad (
 adId               VARCHAR2(8) NOT NULL,
 adAdPlanId         VARCHAR2(8) NOT NULL,
 adHotelId          VARCHAR2(5) NOT NULL,
 adStatus           VARCHAR2(1) ,
 adPayDate          DATE,
 adPic              BLOB,
 adPicContent      VARCHAR2(150),
 adHit              NUMBER(4) ,
 
 CONSTRAINT Ad_adId_PK PRIMARY KEY (adId));
CREATE SEQUENCE Ad_seq
INCREMENT BY 1
START WITH 10000001
NOMAXVALUE;
INSERT INTO Ad (adId,adAdPlanId,adHotelId,adStatus,adPayDate,adPicContent,adHit)VALUES (Ad_seq.NEXTVAL,10000001,10001,4,sysdate,'http://www.evergreen-hotels.com/',50);
