/* 計算評價總人數與結果 */
UPDATE hotel
SET hotelRatingTotal = (SELECT COUNT(ordRatingStarNo)  FROM ord WHERE ordHotelId = 10001)
WHERE hotelId = 10001;

UPDATE hotel
SET hotelRatingResult = (SELECT ROUND(AVG(NVL(ordRatingStarNo,0)),0)
                         FROM (SELECT * FROM ord WHERE ordHotelId = 10001)
                         GROUP BY ordHotelId)
WHERE hotelId = 10001;
--select hotelId, hotelRatingTotal, hotelRatingResult from hotel where hotelId = 10001;

UPDATE hotel
SET hotelRatingTotal = (SELECT COUNT(ordRatingStarNo)  FROM ord WHERE ordHotelId = 10002)
WHERE hotelId = 10002;

UPDATE hotel
SET hotelRatingResult = (SELECT ROUND(AVG(NVL(ordRatingStarNo,0)),0)
                         FROM (SELECT * FROM ord WHERE ordHotelId = 10002)
                         GROUP BY ordHotelId)
WHERE hotelId = 10002;
--select hotelId, hotelRatingTotal, hotelRatingResult from hotel where hotelId = 10002;


commit;


