/* Banner方案 */
CREATE TABLE adPlan(
 adPlanId             VARCHAR2(8) NOT NULL,
 adPlanName           VARCHAR2(90),
 adPlanStartDate      DATE,
 adPlanEndDate        DATE,
 adPlanPrice          Number(6),
 adPlanRemainNo       Number(2),
 CONSTRAINT adPlan_adPlanId_PK PRIMARY KEY (adPlanId)
 );
CREATE SEQUENCE adPlan_seq
INCREMENT BY 1
START WITH 10000001
NOMAXVALUE;
INSERT INTO adPlan (adPlanId,adPlanName,adPlanStartDate,adPlanEndDate,adPlanPrice,adPlanRemainNo) VALUES (adPlan_seq.NEXTVAL,'11月方案',to_date('11 1,2016','MM DD,YYYY'), to_date('11 30,2016','MM DD,YYYY'),5000, 0);
