/* 聊天室 */
CREATE TABLE chat(
 chatId   VARCHAR2(8) NOT NULL,
 chatName VARCHAR2(30),
 CONSTRAINT chat_chatId_pk PRIMARY KEY (chatId)
);
CREATE SEQUENCE chat_seq
INCREMENT BY 1
START WITH 10000001
NOMAXVALUE
NOCYCLE
NOCACHE;
INSERT INTO chat		
VALUES(chat_seq.NEXTVAL, null);		
