<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.memchat.model.MemChatVO" table="memChat">
<!-- 	參考文件:	https://docs.jboss.org/hibernate/orm/3.5/reference/en/html/components.html -->
		<composite-id name="id" class="com.memchat.model.MemChatVO">
			<key-property name="memChatMemId" />
			<key-property name="memChatChatId" />
		</composite-id>

		<property name="memChatDate" column="memChatDate" />
		<property name="memChatContent" column="memChatContent" />
		<property name="memChatPic" column="memChatPic" />
		<property name="memChatStatus" column="memChatStatus" />

	</class>
</hibernate-mapping>

